{
  "scripts": {
    "build": "npm-run-all build:*",
    "build:html": "resumed --theme jsonresume-theme-elegant-maksymgendin --output public/index.html && cp public/index.html public/404.html",
    "build:pdf": "resumed --theme jsonresume-theme-paper --output public/print.html",
    "test": "resumed validate"
  },
  "dependencies": {
    "jsonresume-theme-elegant-maksymgendin": "^1.4.0",
    "jsonresume-theme-paper": "^0.5.0",
    "resumed": "^6.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
